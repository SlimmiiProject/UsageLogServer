{
  "compilerOptions": {
    "target": "esnext" ,
    "experimentalDecorators": true ,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "rootDir": "./src",
    "moduleResolution": "node",
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "typeRoots": ["./src/types", "./node_modules/@types"]
  }
}